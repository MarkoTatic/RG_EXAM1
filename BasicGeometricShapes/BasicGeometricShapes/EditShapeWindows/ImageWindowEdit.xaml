<Window x:Class="BasicGeometricShapes.EditShapeWindows.ImageWindowEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BasicGeometricShapes.EditShapeWindows"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="ImageWindowEdit" Height="160" Width="260">
    <Grid>
        <Label Content="Choose image:" HorizontalAlignment="Left" Height="28" Margin="10,22,0,0" VerticalAlignment="Top" Width="90" RenderTransformOrigin="0.468,-1.328"/>
        <Button Content="Choose" HorizontalAlignment="Left" Margin="135,22,0,0" VerticalAlignment="Top" Width="98" Height="28" Click="Button_Click"/>
        <Button Content="Edit" HorizontalAlignment="Left" Height="29" Margin="20,69,0,0" VerticalAlignment="Top" Width="98" Click="Button_Click_1" />
        <Button Content="Cancel" HorizontalAlignment="Left" Height="29" Margin="135,69,0,0" VerticalAlignment="Top" Width="98" Click="Button_Click_2"/>

    </Grid>
</Window>
